<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xuehuiniaoyu.github.oxpecker">

    <application
        android:hardwareAccelerated="true"
        android:largeHeap="true"
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/oxpecker"
        android:label="@string/app_name"
        android:roundIcon="@drawable/oxpecker"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CustomActivity" />
        <activity android:name=".activity.HelloWorldActivity" />
        <activity android:name=".activity.TestDownloadActivity" />
        <activity android:name=".activity.ImgShowActivity" />
        <activity android:name=".activity.ConstantActivity" />
    </application>

</manifest>